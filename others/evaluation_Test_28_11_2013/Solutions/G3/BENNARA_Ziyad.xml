<?xml version="1.0" encoding="UTF-8" standalone="no"?><Solution evaluation="Eval1" exercice="Eval1" nom="BENNARA_Ziyad_5">
	
	
	
	<Dec>
		<Var nom="i" taille="1" type="1"/>
