<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<Solution evaluation="Eval1" exercice="Eval1" nom="GHENDIRE_Souhail">
	
	
	
	<Dec><Var nom="i" taille="1" type="1"/><Var nom="Tab" taille="10" type="4"/></Dec>
	<Body>
		<For cond="i&lt;10" pas="1" valInitiale="0" var="i">
			<BeginFor><exp left="i" name="exp1" op="&lt;" rangLeft="" rangRight="" right="10"/></BeginFor>
			<If avecSion="1" cond="Tab[i]+1==Tab[i+1]">
			<BeginIf>
				<exp left="exp2" name="exp3" op="==" rangLeft="" rangRight="exp1" right="Tab"/>
				<exp left="i" name="exp1" op="+" rangLeft="" rangRight="" right="1"/>
				<exp left="Tab" name="exp2" op="+" rangLeft="i" rangRight="" right="1"/>
			</BeginIf>
					<Write range="" value="Les elements du tableau sont consecutifs"/>
				<Else>
				<Write range="" value="Les elements du tableau ne sont pas consecutifs"/>
				</Else>
				<EndIf/>
			</If>
		<EndFor/>
		</For></Body></Solution>